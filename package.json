{
	"$schema": "https://www.raycast.com/schemas/extension.json",
	"name": "lgtmoon",
	"title": "lgtmoon",
	"description": "lgtmoon in Raycast.",
	"icon": "lgtm-icon.png",
	"author": "umaidashi",
	"owner": "oidon",
	"categories": ["Developer Tools"],
	"license": "MIT",
	"commands": [
		{
			"name": "recent",
			"title": "Recent LGTM",
			"description": "Show 'Recent' lgtmoon.",
			"mode": "view"
		},
		{
			"name": "random",
			"title": "Random LGTM",
			"description": "Show 'Random' lgtmoon.",
			"mode": "view"
		},
		{
			"name": "history",
			"title": "History LGTM",
			"description": "Show 'History' lgtmoon.",
			"mode": "view"
		}
	],
	"dependencies": {
		"@raycast/api": "^1.69.0",
		"@raycast/utils": "^1.18.1"
	},
	"devDependencies": {
		"@raycast/eslint-config": "^1.0.6",
		"@types/node": "20.8.10",
		"@types/react": "18.2.27",
		"eslint": "^8.51.0",
		"prettier": "^3.0.3",
		"typescript": "^5.2.2"
	},
	"scripts": {
		"build": "ray build -e dist",
		"dev": "ray develop",
		"fix-lint": "ray lint --fix",
		"lint": "ray lint",
		"publish": "npx @raycast/api@latest publish"
	}
}
